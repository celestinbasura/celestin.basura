public class P0303 {

    public static void main(String[] args) {

        int i;
        int k;

        i = 0;

        while (i < 5) {
            i = i + 1;
            k = i * i;
            System.out.printf("Kvadrat broja %d je %d \n", i, k);
        }

    }
}