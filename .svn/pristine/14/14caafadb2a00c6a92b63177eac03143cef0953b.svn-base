package Poglavlje02.Rijesenja;

/**
 * Vje�ba V0204
 * Napisati program koji vr�i konverziju temperature iz Fahrenheit skale u
 * Celziusovu skalu.
 * Formula konverzije je: C = ( F - 32 ) * 5/9
 * Npr: Temperatura od 60.0[F] jednaka je temperaturi od 15.55555555555[C]
 */
public class V0204 {

    public static void main(String[] args) {

        double temperaturaCelsius = 0;
        double temperaturaFarenheit = 60;

        temperaturaCelsius = (temperaturaFarenheit - 32) * 5.0 / 9.0;

        System.out.println("Temperatura od "
                + temperaturaFarenheit + "[F] jednaka je temperaturi od "
                + temperaturaCelsius + "[C]");

    }
}
