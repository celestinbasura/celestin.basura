package Poglavlje10.Vjezbe.GeometrijskiLik;

/**
 *
 * @author cbasura
 */
public class Pravokutnik extends GeomentrijskiLik{
    
    private double a;
    private double b;
    
    public Pravokutnik(double a, double b){
        
        this.a = a;
        this.b = b;       
        
    }
    
    @Override
    public double racunajOpseg(){
        
        return 2 * (a + b);
    }
    
    @Override
    public double racunajPovrsinu(){
        
        return a * b;
    }
   
}
