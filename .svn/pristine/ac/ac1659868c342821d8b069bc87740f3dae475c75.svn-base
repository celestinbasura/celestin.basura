package Poglavlje02.Rijesenja;

/**
 * Vjeï¿½ba VB0204
 * Napisati program koji ispisuje rezultat cjelobrojnog dijeljenja i ostatak
 * dijeljenja dvije varijable tipa long.
 */
public class VB0204 {

    public static void main(String[] args) {

        long a = 27;
        long b = 8;

        System.out.println("Dijeljenje: " + (a / b));
        System.out.println("Ostatak: " + (a % b));

    }
}
