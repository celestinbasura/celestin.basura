package Poglavlje04.Rjesenja;

/**
 * Zadatak Z0401
 * Napisati �etiri metode: zbroji, oduzmi, dijeli, mnozi koje primaju dva broja
 * te vra�aju rezultat navedene operacije. Testirati napisane metode u main
 * metodi pozivajući ih sa proizvoljnim parametrima.
 */
class Z0401 {

    public static void main(String[] args) {

        System.out.println("Zbrajanje: " + zbroji(2, 3));
        System.out.println("Oduzimanje: " + oduzmi(2, 3));
        System.out.println("Mnozenje: " + mnozi(2, 3));
        System.out.println("Dijeljenje: " + dijeli(2, 3));

    }

    static int zbroji(int a, int b) {
        return a + b;
    }

    static int oduzmi(int a, int b) {
        return a - b;
    }

    static double mnozi(double a, double b) {
        return a * b;
    }

    static double dijeli(double a, double b) {
        return a / b;
    }
}
