/**
 * P0291 Primjer switch case naredbe, falltrough svojstvo
 */
public class P0291 {

    public static void main(String[] args) {

        int i = 6;

        switch (i) {
            case 1:
                System.out.println("Broj je 1");
            case 2:
                System.out.println("Broj je 2");
            case 3:
                System.out.println("Broj je 3");
            default:
                System.out.println("Broj je veci od 4");                
            case 4:
                System.out.println("Broj je 4");

        }

    }
}
