package Poglavlje02.Rijesenja;

/**
 * Vjeï¿½ba VB0205
 * Napisati program koji za zadani broj ispisuje da li je pozitivan ili negativan.
 */
public class VB0205 {

    public static void main(String[] args) {

        int a = 7;
        boolean brojJePozitivan;

        brojJePozitivan = (a > 0);

        if(brojJePozitivan) {
            System.out.println("broj je pozitivan");
        }
        else {
            System.out.println("broj nije pozitivan");
        }

    }
}
