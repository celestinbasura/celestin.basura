/**
 * P0241
 * Primjer deklaracije i inicijalizacije varijable i konstante
 */
public class P0241 {

    public static void main(String[] args) {

        int i;		// deklaracija lokalne varijable i
        i = 3;		// inicijalizacija lokalne varijable i

        int j = 3;	// deklaracija i inicijalizacija lokalne varijable j

        final int IME_KONSTANTE;	// deklaracija konstante, modifikator final
        IME_KONSTANTE = 6;		// inicijalizacija konstante

    }
}