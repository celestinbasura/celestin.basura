package Poglavlje03.Rijesenja;

/**
 * Vje�ba V0301
 * Napisati program koji ispisuje sve brojeve od 1 do 10: Uzlazno koriste�i
 * petlju while i silazno koriste�i petlju do-while.
 */
public class V0301 {

    public static void main(String[] args) {

        int i = 1;

        System.out.println("\n --- Uzlazno od 1 do 10 ---");
        while(i <= 10) {
            System.out.printf("%d, ", i);
            i++;
        }

        System.out.println("\n --- Silazno od 10 do 1 ---");
        do {
            i--;
            System.out.printf("%d, ", i);
        } while(i > 1);

    }
}
