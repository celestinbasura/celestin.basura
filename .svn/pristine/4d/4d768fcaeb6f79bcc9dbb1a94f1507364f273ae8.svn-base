package Poglavlje07.Rjesenja;

/**
 * vježba vb0601
 * Napisati program koji ispisuje sve elemente polja integera duljine 10.
 * Potrebno je deklarirati i inicijalizirati polje, te pomoću
 * inicijalizacijskog bloka u polje pospremiti brojeve od 1 do 10.
 */
public class VB0701 {

    public static void main(String[] args) {

        int[] polje = {2, 4, 6, 4, 9, 7, 4, 1, 0, 8};

        System.out.print("[");
        for(int i = 0; i < polje.length; i++) {
            System.out.print(polje[i] + " ");
        }
        System.out.print("]");

    }
}
