package Poglavlje09.Zadaci.Z0901;

/**
 *
 * @author Celestin
 */
public class Mornar extends Osoba{
    
    static String pozicija = "Mornar";
    public void init(){
        this.pozicija = "Mornar";        
    }
    @Override
    public String toString(){
        
        
        StringBuilder sb = new StringBuilder();
        String noviString;
      
        sb.append(this.getIme()).append(" ").append(this.getPrezime()).append(" ").append(pozicija);
        noviString = sb.toString();
        return noviString;
    }

}
